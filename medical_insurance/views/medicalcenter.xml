<odoo>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="medical_center_list">
        <field name="name">MedicalCenter list</field>
        <field name="model">medical_insurance.medical_center</field>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
            <tree>
                  <field name="name"/>
                  <field name="email"/>
                  <field name="phone"/>
                  <field name="mobile"/>
                  <field name="country_id"/>
                  <field name="contact_address"/>

                

            </tree>
        </field>
    </record>



    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="MedicalCenter_action_window">
      <field name="name">MedicalCenter window</field>
      <field name="res_model">medical_insurance.medical_center</field>
      <field name="view_mode">tree,form</field>
    </record>

     <!-- Top menu item -->
    <menuitem name="medical_insurance" id="medical_insurance_menu_root"/>
    <!-- menu categories -->

    <menuitem name="Configuration" id="medical_insurance_menu_3" parent="medical_insurance_menu_root"/>
    <!-- actions -->

    <menuitem name="MedicalCenter" id="medical_insurance_menu_3_list" parent="medical_insurance_menu_3"
              action="MedicalCenter_action_window"/>





</odoo>