<odoo>
     <record model="ir.ui.view" id="medical_insurance_patient_list">
           <field name="name">medical_insurance list</field>
          <field name="model">medical_insurance.patient</field>
          <field name="arch" type="xml">

            <tree>
              <field name="name"/>
              <field name="NID"/>
              <field name="date_of_birth"/>
              <field name="gender"/>
              <field name="marital_status"/>
              <field name="phone"/>
              <field name="email"/>
              <field name="blood_group"/>
              <field name="height"/>
              <field name="weight"/>
              <field name="status"/>

            </tree>
          </field>
        </record>



        <record model="ir.ui.view" id="patient_form">
          <field name="name">patient form</field>
          <field name="model">medical_insurance.patient</field>
          <field name="arch" type="xml">
            <form>
              <!--sheet-->
              <notebook position="inside">
                <page string="Personal Info">
                  <group>
                    <field name="name"/>
                    <field name="NID"/>
                    <field name="date_of_birth"/>
                    <field name="gender"/>
                    <field name="marital_status"/>
                  </group>
                </page>
                <page string="Medical Info">
                  <group>
                  <field name="blood_group"/>
                  <field name="height"/>
                  <field name="weight"/>
                  <field name="status"/>
                  </group>
                </page>
                <page string="Contact Info">
                  <group>
                  <field name="phone"/>
                  <field name="email"/>
                  </group>
                </page>
              </notebook>
            </form>
          </field>

        </record>

        <record model="ir.actions.act_window" id="medical_insurance_action_patient">
          <field name="name">patient window</field>
          <field name="res_model">medical_insurance.patient</field>
          <field name="view_mode">tree,form</field>
        </record>



        <!-- Top menu item -->

        <menuitem name="medical_insurance" id="medical_insurance_menu_root"/>

        <!-- menu categories -->

        <!--menuitem name="Registration" id="medical_insurance_registration" parent="medical_insurance_menu_root"/-->

        <!-- actions -->

        <menuitem name="Patient" id="medical_insurance_patient" parent="medical_insurance_Registration"
                  action="medical_insurance_action_patient"/>
       <!-- <menuitem name="Server to list" id="medical_insurance" parent="medical_insurance.menu_2"
                  action="medical_insurance.action_server"/>
        -->

</odoo>