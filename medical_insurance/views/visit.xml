<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="medical_insurance_list_visit">
      <field name="name">visit list</field>
      <field name="model">medical.insurance.visit</field>
      <field name="arch" type="xml">
        <tree>
           <field name="id"/>
          <field name="patient_id"/>
          <!--field name="date_of_visit"/-->
          <field name="visit_state"/>
        </tree>
      </field>
    </record>


    <!-- explicit form view definition -->

    <record model="ir.ui.view" id="medical_insurance_form_visit">
      <field name="name">visit form</field>
      <field name="model">medical.insurance.visit</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group string="Visit">
              <field name="patient_id"/>
              <field name="medical_center_id"/>
              <field name="service_line_id"/>
              <field name="visit_type"/>
              <field name="patient_status"/>
              <field name="price_plan"/>
              <field name="plan_status"/>
              <field name="contribution_charge"/>
              <field name="patient_charge"/>
              <field name="visit_state"/>
              <!--field name="date_of_visit"/-->
            </group>
          </sheet>
        </form>
      </field>
    </record>


    <!-- explicit graph view definition -->

    <!--record model="ir.ui.view" id="medical_insurance_graph_visit">
      <field name="name">visit graph</field>
      <field name="model">medical.insurance.visit</field>
      <field name="arch" type="xml">
        <graph>
          <field name="date_of_visit"/>
          <field name="patient_id" type="col"/>
        </graph>
      </field>
    </record-->


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="medical_insurance_action_visit">
      <field name="name">visit window</field>
      <field name="res_model">medical.insurance.visit</field>
      <!--field name="view_mode">tree,form,graph</field-->
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
    <!--
    <record model="ir.actions.server" id="medical_insurance.action_server">
      <field name="name">medical_insurance server</field>
      <field name="model_id" ref="model_medical_insurance_medical_insurance"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
    -->

    <!-- Top menu item -->

    <!--menuitem name="medical_insurance" id="medical_insurance_menu_root"/-->

    <!-- menu categories -->

    <!--menuitem name="Registration" id="medical_insurance_Registration" parent="medical_insurance_menu_root"/-->

    <!-- actions -->

    <menuitem name="Visit" id="medical_insurance_visit" parent="medical_insurance_menu_registration"
              action="medical_insurance_action_visit"/>


  </data>
</odoo>