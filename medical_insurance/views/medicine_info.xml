<odoo>
    <record model="ir.ui.view" id="medicine_info_list">
        <field name="name">Medicine Info list</field>
        <field name="model">medical.insurance.medicine_info</field>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
            </tree>
        </field>
    </record>

<!-- explicit form view definition-->
    <record model="ir.ui.view" id="disease_info_form">
        <field name="name">Medicine_info_list</field>
        <field name="model">medical.insurance.medicine_info</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group col="4">
                            <field name="name"/>

                    </group>
                </sheet>
            </form>
        </field>
    </record>
<!-- explicit list view definition -->

<!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="medicine_info_action_window">
      <field name="name">Disease Info window</field>
      <field name="res_model">medical.insurance.medicine_info</field>
      <field name="view_mode">tree,form</field>
    </record>

</odoo>