<openerp>

<data>

    <report

         id="price_plan_report"
        model="medical.insurance.price.plan"
        string="Price Plan Report"

        report_type="qweb-pdf"
        name="medical_insurance.price_plan_report_view"
        file="account.report_invoice"
        />




    <template id="price_plan_report_view">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <t t-call="web.external_layout">
                    <div class="page">

                        <h3>price plan  </h3>
                        <h5 t-field="doc.product_id"/>
                        <h3>status: </h3>
                        <h5 t-field="doc.status"/>
                        <h3>start_date:</h3>
                        <h5 t-field="doc.start_date"/>
                        <h3>end_date: </h3>
                        <h5 t-field="doc.end_date"/>
                    </div>
                </t>
            </t>
        </t>
    </template>

</data>

</openerp>