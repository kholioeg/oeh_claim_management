<odoo>
    <record model="ir.ui.view" id="operation_reservation_list">
        <field name="name">Operation Reservation list</field>
        <field name="model">medical.insurance.operationreservation</field>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
            <tree>
                        <field name="name"/>
                        <field name="From"/>
                        <field name="To"/>
                        <field name="Notes"/>
                        <field name="doctor_name"/>
                        <field name="patient_id"/>

            </tree>
        </field>
    </record>

     <record model="ir.sequence" id="seq_operation_id">
        <field name="name">Operation Number</field>
        <field name="code">medical.insurance.operationreservation</field>
        <field name="prefix">OP</field>
        <field name="padding">5</field>
    </record>

<!-- explicit form view definition-->
    <record model="ir.ui.view" id="operation_reservation_form">
        <field name="name">operation_reservation_list</field>
        <field name="model">medical.insurance.operationreservation</field>
        <field name="arch" type="xml">
            <form>
                <sheet>


                    <group col="4">
                        <field name="name"/>
                        <field name="From"/>
                        <field name="To"/>
                        <field name="Notes"/>
                        <field name="doctor_name"/>
                         <field name="patient_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
<!-- explicit list view definition -->

<!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="Operation_Reservation_action_window">
      <field name="name">Operation Reservation window</field>
      <field name="res_model">medical.insurance.operationreservation</field>
      <field name="view_mode">tree,form</field>
    </record>

</odoo>